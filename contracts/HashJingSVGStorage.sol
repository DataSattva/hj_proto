pragma solidity ^0.8.25;

import "./SSTORE2.sol";

contract HashJingSVGStorage {
    address public immutable svgPointer;

    constructor() {
        svgPointer = SSTORE2.write(hex"");
    }

    function getPointer() external view returns (address) {
        return svgPointer;
    }
}
